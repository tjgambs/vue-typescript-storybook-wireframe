    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "storybook": "^5.3.9",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
